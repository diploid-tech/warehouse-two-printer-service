<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <DebugType>full</DebugType>
    <AssemblyName>Avanti.WarehouseTwoPrinterService</AssemblyName>
    <PackageId>OrderService</PackageId>
    <CodeAnalysisRuleSet>../../AnalysisRules.ruleset</CodeAnalysisRuleSet>
    <RootNamespace>Avanti.WarehouseTwoPrinterService</RootNamespace>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
    <TreatSpecificWarningsAsErrors />
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <AdditionalFiles Include="../../stylecop.json" />
    <EmbeddedResource Include="**\*.sql" />
  </ItemGroup>
  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="avanti-service.yaml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Avanti.Core.EventStream.Azure" Version="1.0.3-g41832fdc51" />
    <PackageReference Include="Avanti.Core.KeyStore.Azure" Version="1.0.0-g37f9aaebcf " />
    <PackageReference Include="Avanti.Core.Microservice" Version="1.2.0-g78ea952c75" />
    <PackageReference Include="Avanti.Core.RelationalData.PostgreSQL" Version="1.1.0-gd7936da2b3" />
  </ItemGroup>
</Project>
